{% extends 'base.html.twig' %}

{% block title %}Jobtitle index{% endblock %}

{% block body %}
<div class="container">
    <h1>Jobtitle index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for jobtitle in jobtitles %}
            <tr>
                <td>{{ jobtitle.id }}</td>
                <td>{{ jobtitle.title }}</td>
                <td>{{ jobtitle.createdAt ? jobtitle.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ jobtitle.updatedAt ? jobtitle.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('back_jobtitle_show', {'id': jobtitle.id}) }}">show</a>
                    <a href="{{ path('back_jobtitle_edit', {'id': jobtitle.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('back_jobtitle_new') }}">Create new</a>
</div>
{% endblock %}
