<form method="post" action="{{ path('back_experience_delete', {'id': experience.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ experience.id) }}">
    <button class="btn btn-danger btn-sm">Supprimer</button>
</form>
