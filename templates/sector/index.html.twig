{% extends 'base.html.twig' %}

{% block title %}Sector index{% endblock %}

{% block body %}
    <h1>Sector index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>SectorName</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sector in sectors %}
            <tr>
                <td>{{ sector.id }}</td>
                <td>{{ sector.sectorName }}</td>
                <td>{{ sector.createdAt ? sector.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ sector.updatedAt ? sector.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('back_sector_show', {'id': sector.id}) }}">show</a>
                    <a href="{{ path('back_sector_edit', {'id': sector.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('back_sector_new') }}">Create new</a>
{% endblock %}
